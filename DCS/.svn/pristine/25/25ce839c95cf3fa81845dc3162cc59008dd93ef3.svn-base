		function idVal(id){
			return document.getElementById(id).value;
		}
				
		function saveVal(num, text) {

		    if (num > 2) {
		        $('#onetext').text(text + "--低");
		        $('#twotext').text(text + "--中");
		        $('#threetext').text(text + "--高");
		    }
		    else {
		        $('#onetext').text(text);
		    }
			var obj_1_from_cookile = cookieUtil.get("obj_1_to_string_val");
		
			if(obj_1_from_cookile != '' && obj_1_from_cookile != null){
				var obj = JSON.parse(obj_1_from_cookile);
				$("#v_1_1").val(obj.v_1_1);
