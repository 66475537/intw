            function upFun(obj){
            	var $up = $(obj); 
  						var len = $up.length;
  						var $tr = $(obj).parents("tr"); 
					    if ($tr.index() != len - 1) { 
					      $tr.fadeOut().fadeIn(); 
      					$tr.prev().before($tr);  
					    }
  						
            }
            
            function downFun(obj){
            	var $down = $(obj); 
  						var len = $down.length;
  						var $tr = $(obj).parents("tr"); 
					    if ($tr.index() != len + 1) { 
					      $tr.fadeOut().fadeIn(); 
					      $tr.next().after($tr); 
					    }
  						
            }
            
            function deleteRowFun(obj){
            	var $tr = $(obj).parents("tr"); 
   					 	$tr.remove(); 
            }
            
