@Controller
@Scope(value = "prototype")
@RequestMapping(value = "/terminal")
public class TerminalController {
    private Logger logger = LoggerFactory.getLogger(TerminalController.class);
    @Autowired
    private TerminalService terminalService;

    /**
     * 展示终端要素
     *
     * @param model
     * @return
     */
    @RequestMapping(value = "/displayTerminalTree.do")
    public String displayTerminalTree(Integer flag, Model model) {
        return null ==flag || flag == 3 ? "terminal/terminalTree" : "terminal/terminalTree2";
    }
}
